<div class="col-sm-1 col-sm-6 col-lg-4" data-list-id='<%= list.id %>'> 
  <%= link_to list_path(list) do %>
    <div class="card-category my-3" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')">
      <%= list.name %>
    </div>
  <% end %>
</div> 